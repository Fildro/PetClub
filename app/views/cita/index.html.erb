<p id="notice"><%= notice %></p>

<h1>Listing Cita</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Obs</th>
      <th>Mascota</th>
      <th>Medico</th>
      <th>Cliente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cita.each do |citum| %>
      <tr>
        <td><%= citum.date %></td>
        <td><%= citum.time %></td>
        <td><%= citum.obs %></td>
        <td><%= citum.mascota %></td>
        <td><%= citum.medico %></td>
        <td><%= citum.cliente %></td>
        <td><%= link_to 'Show', citum %></td>
        <td><%= link_to 'Edit', edit_citum_path(citum) %></td>
        <td><%= link_to 'Destroy', citum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Citum', new_citum_path %>
