<p id="notice"><%= notice %></p>
 <div class="col-sm-12">
<h1>Listado de Servicios</h1>

<table class="table table-bordered table-hover table-striped">
 <div class="table-responsive">
  <thead>
    <tr>
      <th>Tipo Servicio</th>
      <th>Descripci√≥n</th>
      <th>Precio</th>
      <th>Ver</th>
      <th>Editar</th>
      <th>Eliminar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @servicios.each do |servicio| %>
      <tr>
        <td><%= servicio.tipose %></td>
        <td><%= servicio.descripcionse %></td>
        <td><%= servicio.preciose %></td>
        <td><%= link_to 'Ver', servicio %></td>
        <td><%= link_to 'Editar', edit_servicio_path(servicio) %></td>
        <td><%= link_to 'Eliminar', servicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>


<br>
<%= link_to 'Registrar Nuevo Servicio', new_servicio_path %>
<br>
<%=will_paginate @servicios %>