<p id="notice"><%= notice %></p>

<h1>Listing Mascota</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Color</th>
      <th>Fechanac</th>
      <th>Sexo</th>
      <th>Peso</th>
      <th>Cliente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mascota.each do |mascotum| %>
      <tr>
        <td><%= mascotum.nombre %></td>
        <td><%= mascotum.color %></td>
        <td><%= mascotum.fechanac %></td>
        <td><%= mascotum.sexo %></td>
        <td><%= mascotum.peso %></td>
        <td><%= mascotum.cliente %></td>
        <td><%= link_to 'Show', mascotum %></td>
        <td><%= link_to 'Edit', edit_mascotum_path(mascotum) %></td>
        <td><%= link_to 'Destroy', mascotum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mascotum', new_mascotum_path %>
